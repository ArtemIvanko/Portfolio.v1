{
    "extends": ["stylelint-config-recommended"],
    "rules": {
      "no-empty-source": null,
      "declaration-empty-line-before": null,
      "no-missing-end-of-source-newline": null,
      "function-no-unknown": null
    },
    "overrides": [
      {
        "files": ["**/*.{js,ts,jsx,tsx}"],
        "customSyntax": "@stylelint/postcss-css-in-js"
      }
    ]
  }
  